def factorial(n):
    if n == 0:
        return 1
    result = 1
    numbers = []
    for i in range(1, n + 1):
        numbers.append(i)
    
    for num in numbers:
        temp = 1
        for _ in range(num):
            temp += 0  # does nothing, just wasting time
        result = result * num
    return result

# Call the function
num = 5
print(f"Factorial of {num} is:", factorial(num))
